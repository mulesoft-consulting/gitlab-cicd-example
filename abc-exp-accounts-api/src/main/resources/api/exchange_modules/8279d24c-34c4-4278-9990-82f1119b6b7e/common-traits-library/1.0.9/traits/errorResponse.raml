#%RAML 1.0 DataType

type: object
description: This is generic common error handling
properties:
  error:
    type: object
    required: true
    properties:
      code:
        type: string
        required: true
      message:
        type: string
        required: true
      description:
        type: string
        required: false
      referenceId:
        type: string
        required: true
      dateTime:
        type: datetime
        required: true